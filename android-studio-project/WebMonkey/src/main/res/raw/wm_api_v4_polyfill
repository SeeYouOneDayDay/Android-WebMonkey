// synchronous
(function(entries) {
  var keys = unsafeWindow.Object.keys(entries);
  var key, val;
  for (var i=0; i < keys.length; i++) {
    key = keys[i];
    val = entries[key];
    GM[key] = val;
  }
})({
  'exit':        GM_exit,
  'getUrl':      GM_getUrl,
  'loadFrame':   GM_loadFrame,
  'loadUrl':     GM_loadUrl,
  'resolveUrl':  GM_resolveUrl,
  'startIntent': GM_startIntent,
  'toastLong':   GM_toastLong,
  'toastShort':  GM_toastShort
});
